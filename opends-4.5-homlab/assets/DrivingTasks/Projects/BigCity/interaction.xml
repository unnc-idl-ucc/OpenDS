	<!--   dump location
	
		<entry>1800.0</entry><entry>0.0</entry><entry>1800.0</entry>
	-->
	
	<triggers>
	
		<trigger id="collide_WayPoint_51_trigger_1" priority="1">
				<activities>
					<activity id="remove_WayPoint_54_wall_1">
						<action id="manipulateObject" delay="20" repeat="0">
							<parameter name="id" value="WayPoint_54_wall_1" />
							<parameter name="visible" value = "false" />
							<parameter name="setTranslationX" value = "1800.0" />
							<parameter name="setTranslationY" value = "0.0" />
							<parameter name="setTranslationZ" value = "1800.0" />
						</action>
					</activity>
			
					<activity id="askForGreen_WayPoint_54_TrafficLight_1">
					  <action id="requestGreenTrafficLight" delay="18" repeat="0">
						<parameter name="trafficLightID" value = "WayPoint_54_TrafficLight_1" />
					  </action>
					</activity>

					
				</activities>
				<condition>collideWith:WayPoint_51_trigger_1</condition>    
		</trigger>

		
		
			
