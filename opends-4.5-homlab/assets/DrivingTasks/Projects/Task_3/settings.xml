<?xml version="1.0" encoding="UTF-8"?><settings xmlns="http://opends.eu/drivingtask/settings" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://opends.eu/drivingtask/settings ../../Schema/settings.xsd">

	<general>
		<driverName>TestDriver</driverName>
		<showStats>false</showStats>
		<captureVideo>_.avi</captureVideo>
	</general>
	
	<objectLocator>
		<enable>true</enable>
		<fileName>trafficObjects.txt</fileName>	
	</objectLocator>
	
	<HMI>
		<enableConnection>false</enableConnection>
		<ip>127.0.0.1</ip>
		<port>2111</port>
	</HMI>
	
	<externalVisualization>
		<enableConnection>false</enableConnection>
		<ip>141.96.189.103</ip>
		<port>4711</port>
		<updateRate>20</updateRate>
		<scalingFactor>100</scalingFactor>
		<sendPosOriAsOneString>false</sendPosOriAsOneString>
	</externalVisualization>
	
	<CANInterface>
		<enableConnection>false</enableConnection>
		<ip>192.168.0.2</ip>
		<port>4711</port>
		<updateRate>20</updateRate>
		<maxSteeringAngle>180</maxSteeringAngle>
	</CANInterface>
	
	<vsimrtiServer>
		<startServer>false</startServer>
		<port>1122</port>
	</vsimrtiServer>

	<shadowCarModel>
		<enableHighPolyCarModel>true</enableHighPolyCarModel>
	</shadowCarModel>
	
	<multiThreading>
       <enableThreads>true</enableThreads>
       <numberOfThreads>13</numberOfThreads>
    </multiThreading>
	
	
	<reactionMeasurement>
		<groupRed>incongruent</groupRed>
		<groupGreen>congruent</groupGreen>
		<groupYellow>control_neutral</groupYellow>
		<groupCyan>control_silence</groupCyan>
		<groupBlue>no_target</groupBlue>
		<groupMagenta/>
	</reactionMeasurement>
	
	<controllers>
		<joystick>
			<!-- Logitech G27 -->
			<dumpJoystickList>true</dumpJoystickList>
			<steering axisID="3" controllerID="0" invert="false" sensitivity="1.0"/>
			<combinedPedals axisID="4" controllerID="0" invert="false" sensitivity="1.0"/>
			<accelerator axisID="6" controllerID="0" invert="true" sensitivity="1.0"/>
			<brake axisID="5" controllerID="0" invert="true" sensitivity="1.0"/>
			<clutch axisID="7" controllerID="0" invert="true" sensitivity="1.0"/>
			<keyAssignments>
				<keyAssignment function="start_engine" key="BUTTON_1"/>
				<keyAssignment function="stop_engine" key="BUTTON_2"/>
			</keyAssignments>
		</joystick>
		<keyboard>
			<keyAssignments>
				<keyAssignment function="toggle_cam" key="KEY_V"/>
				<keyAssignment function="horn" key="KEY_H"/>
			</keyAssignments>
		</keyboard>
		<mouse>
			<!-- only for outside view -->
			<minScrollZoom>1</minScrollZoom>
			<maxScrollZoom>40</maxScrollZoom>
			<scrollSensitivityFactor>5</scrollSensitivityFactor>
		</mouse>
	</controllers>
	
</settings>